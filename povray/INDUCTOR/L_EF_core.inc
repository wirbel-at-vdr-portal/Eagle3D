     pigment {
        rgb <0.3, 0.3, 0.3>
        }
     rotate <90,0,0>
     translate <0,6,0>
     }

  box { <-3,3,3> <3,10,-3> texture {T_Gold_1A } }

  union { // Spulenkoerper
     difference {
