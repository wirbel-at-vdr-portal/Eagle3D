     cylinder { < -5, 0.5,4> < -5,4,4> 1 }
     difference {
        box { <-6, 0.5, -1.75> < 6,4  , -5  > }
        box { < 3, 0.4, -3   > < 1,4.1, -5.1> }
        box { <-1, 0.4, -3   > <-3,4.1, -5.1> }
        cylinder { < 6,-0.4,-5> < 6,4.1,-5> 1 }
        cylinder { <-6,-0.4,-5> <-6,4.1,-5> 1 }
        }
     cylinder { < 5, 0.5,-4> < 5,4,-4> 1 }
     cylinder { <-5, 0.5,-4> <-5,4,-4> 1 }
     difference {
        box { <-5, 10, 5> < 5,11, -5  > }
        box { < 3, 9, -3> < 1,12, -5.1> }
        box { <-1, 9, -3> <-3,12, -5.1> }
        box { < 3, 9,  3> < 1, 12, 5.1> }
        box { <-1, 9,  3> <-3, 12, 5.1> }
