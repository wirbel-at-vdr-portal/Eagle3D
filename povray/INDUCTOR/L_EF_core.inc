        cylinder { < -5, 9, 5> < -5,12, 5> 1 }
        cylinder { <  5, 9,-5> <  5,12,-5> 1 }
        }
     cylinder { < 4, 10, 4> <  4,11, 4> 1 }
     cylinder { <-4, 10,-4> < -4,11,-4> 1 }
     cylinder { <-4, 10, 4> < -4,11, 4> 1 }
     cylinder { < 4, 10,-4> <  4,11,-4> 1 }
     difference {
        box { <-4, 6.5, 4> < 4,7, -4> }
