     // Kern
     union {
        difference {
           box { <-6  ,-1.75,-6  > <6  ,1.75,6  > }
           box { <-4.5,-1.76,-4.5> <4.5,1.76,4.5> }

           cylinder { < 6,-1.76, 6> < 6,1.76, 6> 1 }
